{
  "name": "@livejack/server",
  "version": "0.3.0",
  "dependencies": {
    "@livejack/client": "^1.2.1",
    "@livejack/prosemirror": "^1.2.2",
    "@ungap/custom-elements": "^1.0.0",
    "@webmodule/serve": "^5.2.3",
    "ajv-keywords": "^3.5.2",
    "cors": "^2.8.5",
    "datauri": "^3.0.0",
    "diff-dom": "^4.2.2",
    "diff-list": "^1.0.3",
    "express": "^4.17.1",
    "express-dom": "^5.14.3",
    "express-extension-to-accept": "^0.0.2",
    "express-urlrewrite": "^1.4.0",
    "flatpickr": "^4.6.9",
    "got": "^11.8.2",
    "http-errors": "^1.8.0",
    "knex": "^0.21.19",
    "matchdom": "^7.0.0-beta.16",
    "method-override": "^3.0.0",
    "modern-normalize": "^1.1.0",
    "morgan": "^1.10.0",
    "objection": "^2.2.16",
    "objection-db-errors": "^1.1.2",
    "p-queue": "^6.6.2",
    "passwd-user": "^3.0.0",
    "pg": "^8.7.1",
    "serve-static": "^1.14.1",
    "sharp": "^0.26.3",
    "toggle-switch-css": "^1.0.3",
    "toml": "^3.0.0",
    "upcache": "^2.1.1",
    "url-inspector": "^3.3.3",
    "xbytes": "^1.7.0",
    "xdg-basedir": "^4.0.0"
  },
  "devDependencies": {
    "@kapouer/eslint-config": "^1.6.1",
    "@kapouer/stylelint-config": "^1.1.0",
    "@webmodule/bundle": "^5.5.3",
    "expect.js": "^0.3.1",
    "jsondiffpatch": "^0.4.1",
    "mocha": "^9.1.2"
  },
  "eslintConfig": {
    "extends": "@kapouer/eslint-config",
    "globals": {
      "HttpError": true
    },
    "overrides": [
      {
        "files": [
          "lib/**",
          "models/**",
          "resources/**",
          "routes/**",
          "*.js"
        ],
        "env": {
          "node": true,
          "es6": true,
          "browser": false
        }
      },
      {
        "files": [
          "public/js/*.js",
          "public/js/editor/*.js",
          "public/js/elements/*.js"
        ],
        "env": {
          "browser": true
        }
      }
    ]
  },
  "stylelint": {
    "extends": "@kapouer/stylelint-config"
  },
  "scripts": {
    "start": "NODE_ENV=development node app.js",
    "bundle": "webmodule-bundle --root public --concatenate --css dist/live-${npm_config_name}-${npm_package_version}.css --js dist/live-${npm_config_name}-${npm_package_version}.js public/live-${npm_config_name}.html",
    "bundle-all": "npm run bundle --name=read && npm run bundle --name=write",
    "minify": "webmodule-bundle --root public --css dist/live-${npm_config_name}-${npm_package_version}.min.css --js dist/live-${npm_config_name}-${npm_package_version}.min.js public/live-${npm_config_name}.html",
    "minify-all": "npm run minify --name=read && npm run minify --name=write",
    "dist": "npm run bundle-all && npm run minify-all"
  },
  "browserslist": [
    "> 0.25% in FR",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ]
}
